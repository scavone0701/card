{
    "name": "meuapp",
    "version": "1.0.0",
    "description": "Bot Meu Geladin Gourmet",
    "main": "app.js",
    "author": "Scavone",
    "engines": {
    "node": "14.x"
    },
    "scripts": {
        "start": "node app.js"
    },
    "bin": {
        "meuapp": "app.js"
    },
    "pkg": {
        "scripts": "app.js",
        "targets": [
            "node14-win-x64",
            "node14-linux-x64",
            "node14-macos-x64"
        ]
    },
    "dependencies": {
        "apache": "^0.1.0",
        "axios": "^1.5.1",
        "baileys": "^6.0.9",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "latest-version": "^7.0.0",
        "socket.io": "^4.7.2",
        "venom-bot": "^4.2.1"
    },
    "devDependencies": {
        "electron": "^27.0.0",
        "electron-builder": "^24.6.4"
    },
    "build": {
        "appId": "meu.geladin.gourmet",
        "productName": "MeuGeladinGourmet",
        "files": [
            "dist/**/*",
            "node_modules/**/*"
        ],
        "win": {
            "target": "nsis"
        },
        "mac": {
            "target": "dmg"
        },
        "linux": {
            "target": "AppImage"
        }
    }
}
